{
  "$schema": "https://raw.githubusercontent.com/nicholasgasior/tauri-schema/refs/heads/main/tauri-capability.schema.json",
  "identifier": "default",
  "description": "Default capability for Clickweave",
  "windows": ["main"],
  "permissions": [
    "core:default",
    "core:event:default",
    "dialog:default",
    "dialog:allow-open",
    "dialog:allow-save",
    "shell:default",
    "shell:allow-open",
    "store:default",
    "global-shortcut:allow-register"
  ]
}
